[Unit]
Description=Python daemon
After=syslog.target
After=network.target

[Service]
Type=simple
User=testuser
Group=testuser
PIDFile=/tmp/foo.pid
ExecStart=/usr/bin/python /home/testuser/black-footed/schedule-test.py
StandardOutput=syslog
StandardError=syslog
SyslogIdentifier=test-err
Restart=always
TimeoutStartSec=10
RestartSec=10



[Install]
WantedBy=multi-user.target
